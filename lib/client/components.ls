@D =( cls, ...args)-> div class:cls, args



@B =-> switch it # кнопки
	| \link__back => button class:'btn btn-default back-button' onclick:'history.back()', 'Назад'
	| \big-red    => a class:'btn btn-primary' style:'font-size:30px; margin:5px' href:&1, &2
	| _  => if &1 => button class:'btn btn-default' onclick:&1, &0
		else button class:'btn btn-default', &0

@W =( ...args)-> # W — wrapper
	| \container => div class:\container, args

@I =(cls, ...args)-> # I — иконка 
	i class:"fa fa-#cls", args


@SVG = ->
	| \soviet-star => 'data:image/svg+xml;base64,'





