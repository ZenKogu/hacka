
template(name="Estates")
  div.navbar.navbar-default(style='width:100%; height:70px; z-index:1020;')
      .container-fluid
        //- .navbar-header
        //-   a.navbar-brand(href='/estates' style='display:inline-block') 
        //-     img(style='height:50px' src='data:image/svg+xml;base64,')
        
            //- | Kvartirius 
        .navbar-form.navbar-left(style='margin-left:70px')
          .input-group(style='margin:0px')
            .input-group-addon
              //- a.btn.btn-default(style='width:62px; height:42px')
              i.fa.fa-search(style='font-size:25px')
            input.form-control(style='width:500px; height:70px; padding:0px; margin:0px; font-size:25px' placeholder='Поиск жилья' autofocus)
          .input-group(style='padding:0px;margin:0px')
            .input-group-addon
              i.fa.fa-ruble(style='font-size:25px')
            input.form-control(style='width:100px; height:70px; padding:0px; margin:0px; font-size:25px' placeholder='От')
          .input-group(style='padding:0px;margin:0px')    
            input.form-control(style='width:100px; height:70px; padding:0px; margin:0px; font-size:25px' placeholder='До')


  div.container(style='width:100%')
    +if isAdmin
      a.col-md-3(style=' height:100%; padding-left:0px; padding-right:0px; margin-left:2px;margin-right:2px; width:306px')
        .card(style='width:100%')
          .carousel.slide(id='carousel-{{_id}}' data-ride='carousel' style='width:100%')
            .carousel-inner(role='listbox')
              .item.active
                img        
                i.fa.fa-plus.dataview-insert-button(style='font-size:100px; padding-top:77px; padding-left:103px; text-align:center;')


    +each tableItems
      .col-md-3(style=' height:100%; padding-left:0px; padding-right:0px; margin-left:2px;margin-right:2px; width:306px')
        .card(style='width:100%')
          .carousel.slide(id='carousel-{{_id}}' data-ride='carousel' style='width:100%')
            .carousel-inner(role='listbox')
              .item.active
                img(src='{{gallery_head photos}}', alt='...' style='border-radius: 5px 5px 0 0; width:100%')                   
              +each src in gallery_tail photos
                .item
                  img.img__estate(src='{{src}}' style='border-radius: 5px 5px 0 0; width:100%')                   

            a.left.carousel-control(href='#carousel-{{_id}}', role='button', data-slide='prev' style='border-radius: 5px 5px 0 0; position:absolute;')
              span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
              span.sr-only Далее
            a.right.carousel-control(href='#carousel-{{_id}}', role='button', data-slide='next' style='border-radius: 5px 5px 0 0; position:absolute;')
              span.glyphicon.glyphicon-chevron-right
              span.sr-only Назад

          .card__txt(style='background-color:white; width:306px; height:40px; position:absolute; bottom:0px; border-radius:0 0 5px 5px')
            h5(style='display:inline; vertical-align:text-bottom; position:absolute; padding-left:10px')
              i.fa.fa-ruble
              | {{price}}      
              small(style='font-size:12px; overflow:hidden') {{title}}
          +if isAdmin
            div(style='display:inline; text-align:right; position:absolute; top:6px; right:4px;')
              .btn.btn-default.edit-button(class = '{{editButtonClass}}') 
                i.fa.fa-cog
              .btn.btn-default.delete-button(class = '{{deleteButtonClass}}')
                i.fa.fa-remove
